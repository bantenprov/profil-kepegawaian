<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class Pegawai extends Model
{
	protected $table 	= "pegawais";

	protected $fillable = ['user_id',
							'nip',
							'dinas',
							'full_name',
							'jabatan',
							'join_date',
							'tgl_pensiun',
							'description'
							];

	public function getUser()
	{
		return $this->hasOne('App\User','id');	
	}
}
